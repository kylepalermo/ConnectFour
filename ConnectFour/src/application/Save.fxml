<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="savePane" minHeight="600.0" minWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SaveController">
   <center>
      <VBox fx:id="vBox" alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Label text="Please enter a save name." textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textField" maxWidth="250.0" />
            <Button fx:id="backButton" mnemonicParsing="false" text="Back" />
         </children>
      </VBox>
   </center>
</BorderPane>
